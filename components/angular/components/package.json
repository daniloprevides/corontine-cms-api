    "@angular/elements": "^9.0.7",
    "@angular/forms": "~9.0.7",
    "@angular/localize": "^9.0.7",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@fortawesome/angular-fontawesome": "0.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^6.0.1",
    "add": "^2.0.6",
    "ag-grid-angular": "^23.0.2",
    "ag-grid-community": "^23.0.2",
    "angular-gridster2": "^9.1.0",
    "angular-resizable-element": "^3.3.0",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "concat": "^1.0.3",
    "document-register-element": "^1.7.2",
    "elements-zone-strategy": "^9.0.0",
    "fs-extra": "^9.0.0",
    "jquery": "^3.4.1",
    "npm-watch": "^0.6.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "yarn": "^1.22.4",
    "zone.js": "~0.10.2"
