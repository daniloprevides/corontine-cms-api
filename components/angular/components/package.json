{
  "name": "components",
  "version": "0.0.0",
  "watch": {
    "dev": {
      "patterns": [
        "dist/*"
      ],
      "extensions": "*"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload=false --output-hashing none && node concatenate.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "watch": "npm-watch dev",
    "e2e": "ng e2e",
    "dev": "npm-run-all build:dev copy:folder",
    "copy:folder": "node concatenate_dev.js",
    "build:dev": "ng build --configuration ce --watch",
    "build:deploy": "ng build --prod --output-hashing none && node concatenate_prod.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.7",
